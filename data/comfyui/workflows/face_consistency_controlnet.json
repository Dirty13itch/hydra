{
  "_meta": {
    "name": "Empire Character Consistency (ControlNet)",
    "description": "Generates character portraits with structural consistency using ControlNet canny and CLIPVision style reference",
    "version": "1.0.0",
    "variables": {
      "POSITIVE_PROMPT": "Character description with emotion",
      "NEGATIVE_PROMPT": "Quality negatives",
      "CHARACTER_NAME": "Character name for output",
      "REFERENCE_IMAGE_PATH": "Path to reference character image",
      "CONTROLNET_STRENGTH": "Strength of structure guidance (0.5-0.9)",
      "OUTPUT_PREFIX": "Prefix for saved files"
    },
    "notes": "Uses canny edge detection for facial structure consistency. Reference image should be a clean character portrait."
  },
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "NoobAI-XL-v1.0.safetensors"
    },
    "_meta": {
      "title": "Load SDXL Model"
    }
  },
  "2": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{REFERENCE_IMAGE_PATH}}"
    },
    "_meta": {
      "title": "Load Reference Image"
    }
  },
  "3": {
    "class_type": "Canny",
    "inputs": {
      "image": ["2", 0],
      "low_threshold": 0.1,
      "high_threshold": 0.3
    },
    "_meta": {
      "title": "Extract Canny Edges"
    }
  },
  "4": {
    "class_type": "ControlNetLoader",
    "inputs": {
      "control_net_name": "sdxl_canny.safetensors"
    },
    "_meta": {
      "title": "Load Canny ControlNet"
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{POSITIVE_PROMPT}}, masterpiece, best quality, highres, detailed face, detailed eyes, anime illustration, visual novel style, clean lines, soft lighting, consistent character design",
      "clip": ["1", 1]
    },
    "_meta": {
      "title": "Positive Prompt"
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{NEGATIVE_PROMPT}}, lowres, bad anatomy, bad hands, text, error, missing fingers, extra digit, fewer digits, cropped, worst quality, low quality, jpeg artifacts, signature, watermark, blurry, deformed face, asymmetrical eyes, off-model",
      "clip": ["1", 1]
    },
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "7": {
    "class_type": "ControlNetApplyAdvanced",
    "inputs": {
      "positive": ["5", 0],
      "negative": ["6", 0],
      "control_net": ["4", 0],
      "image": ["3", 0],
      "strength": "{{CONTROLNET_STRENGTH}}",
      "start_percent": 0.0,
      "end_percent": 0.8
    },
    "_meta": {
      "title": "Apply ControlNet"
    }
  },
  "8": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "_meta": {
      "title": "Create Latent"
    }
  },
  "9": {
    "class_type": "KSampler",
    "inputs": {
      "model": ["1", 0],
      "positive": ["7", 0],
      "negative": ["7", 1],
      "latent_image": ["8", 0],
      "seed": -1,
      "steps": 30,
      "cfg": 7.0,
      "sampler_name": "dpmpp_2m_sde",
      "scheduler": "karras",
      "denoise": 1.0
    },
    "_meta": {
      "title": "KSampler"
    }
  },
  "10": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["9", 0],
      "vae": ["1", 2]
    },
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "11": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["10", 0],
      "filename_prefix": "{{OUTPUT_PREFIX}}_{{CHARACTER_NAME}}"
    },
    "_meta": {
      "title": "Save Output"
    }
  },
  "12": {
    "class_type": "PreviewImage",
    "inputs": {
      "images": ["3", 0]
    },
    "_meta": {
      "title": "Preview Canny Edges"
    }
  }
}
