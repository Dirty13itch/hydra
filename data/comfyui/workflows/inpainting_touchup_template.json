{
  "_meta": {
    "name": "Inpainting Touch-up",
    "description": "Fix specific areas of an image using inpainting",
    "version": "1.0.0",
    "variables": {
      "BASE_IMAGE": "Path to image to fix",
      "MASK_IMAGE": "Path to mask (white = areas to fix)",
      "FIX_PROMPT": "Description of what to generate in masked area",
      "OUTPUT_PREFIX": "Output filename prefix"
    },
    "use_case": "Fixing hands, faces, or details in generated images"
  },
  "1": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "NoobAI-XL-v1.0.safetensors"
    }
  },
  "2": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{BASE_IMAGE}}"
    }
  },
  "3": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{MASK_IMAGE}}"
    }
  },
  "4": {
    "class_type": "VAEEncodeForInpaint",
    "inputs": {
      "pixels": ["2", 0],
      "mask": ["3", 0],
      "vae": ["1", 2],
      "grow_mask_by": 8
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{FIX_PROMPT}}, masterpiece, best quality, detailed, anime illustration",
      "clip": ["1", 1]
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "lowres, bad quality, deformed, ugly, blurry",
      "clip": ["1", 1]
    }
  },
  "7": {
    "class_type": "KSampler",
    "inputs": {
      "model": ["1", 0],
      "positive": ["5", 0],
      "negative": ["6", 0],
      "latent_image": ["4", 0],
      "seed": "{{SEED}}",
      "steps": 30,
      "cfg": 7.0,
      "sampler_name": "dpmpp_2m_sde",
      "scheduler": "karras",
      "denoise": 0.8
    }
  },
  "8": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["7", 0],
      "vae": ["1", 2]
    }
  },
  "9": {
    "class_type": "SaveImage",
    "inputs": {
      "images": ["8", 0],
      "filename_prefix": "{{OUTPUT_PREFIX}}_fixed"
    }
  }
}
