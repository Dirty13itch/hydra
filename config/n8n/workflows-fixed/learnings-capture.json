    {
      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "// Format each learning from hydra-health\nconst learning = $input.item.json;\n\nconst markdown = `\n### ${learning.title || 'Auto-captured Learning'}\n**Date:** ${new Date().toISOString().split('T')[0]}  \n**Category:** ${learning.category || 'operations'}  \n**Source:** hydra-health (auto)  \n${learning.tags?.length > 0 ? `**Tags:** ${learning.tags.join(', ')}` : ''}\n\n${learning.content || learning.description || ''}\n\n---\n`;\n\nreturn {\n  json: {\n    id: learning.id,\n    markdown: markdown\n  }\n};"
      },
